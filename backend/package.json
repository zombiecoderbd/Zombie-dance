{
    "name": "zombie-backend",
    "version": "1.0.0",
    "description": "Backend agent server for Zombie AI Assistant",
    "main": "dist/server.js",
    "type": "module",
    "scripts": {
        "dev": "NODE_OPTIONS=--no-deprecation tsx watch src/server.ts",
        "proxy": "NODE_OPTIONS=--no-deprecation tsx watch src/proxy/proxyServer.ts",
        "dev:all": "concurrently \"npm run dev\" \"npm run proxy\"",
        "build": "tsc",
        "start": "NODE_OPTIONS=--no-deprecation node dist/server.js",
        "init-db": "tsx src/scripts/initDb.ts",
        "test": "vitest",
        "lint": "eslint src --ext ts"
    },
    "keywords": [
        "ai",
        "llm",
        "streaming",
        "sse"
    ],
    "author": "Zombie Coder",
    "license": "MIT",
    "dependencies": {
        "better-sqlite3": "^11.0.0",
        "cors": "^2.8.5",
        "diff": "^7.0.0",
        "dotenv": "^16.4.5",
        "express": "^4.21.0",
        "express-rate-limit": "^7.4.0",
        "http-proxy-middleware": "^3.0.3",
        "mysql2": "^3.11.0",
        "openai": "^4.52.0",
        "pino": "^9.2.0",
        "pino-pretty": "^11.2.0",
        "sqlite3": "^5.1.7",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.13",
        "@types/cors": "^2.8.17",
        "@types/diff": "^5.0.8",
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.0",
        "@types/sqlite3": "^3.1.8",
        "@types/ws": "^8.5.10",
        "@typescript-eslint/eslint-plugin": "^6.13.0",
        "@typescript-eslint/parser": "^6.13.0",
        "concurrently": "^9.2.1",
        "eslint": "^8.54.0",
        "tsx": "^4.7.0",
        "typescript": "^5.3.2",
        "vitest": "^1.0.4"
    }
}
